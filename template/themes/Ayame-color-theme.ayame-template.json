    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "[[ayame:colors.comment.hex]]"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "[[ayame:colors.variable.hex]]"
            }
        },
        {
            "name": "Colors",
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "[[ayame:colors.purewhite.hex]]"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "[[ayame:colors.invalid.hex]]"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "[[ayame:colors.keyword.hex]]",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Operator Keyword",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "[[ayame:colors.operator.hex]]",
                "fontStyle": ""
            }
        },
        {
            "name": "Operator, Misc",
            "scope": [
                "constant.other.color",
                "punctuation",
                "meta.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.section.embedded",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "[[ayame:colors.terminator.hex]]"
            }
        },
        {
            "name": "String Definition Punctuation",
            "scope": [
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "[[ayame:colors.green700.hex]]"
            }
        },
        {
            "name": "Template Definition Punctuation",
            "scope": [
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "[[ayame:colors.red.hex]]"
            }
        },
        {
            "name": "Control Keyword",
            "scope": [
                "keyword.control"
            ],
            "settings": {
                "foreground": "[[ayame:colors.control.hex]]",
                "fontStyle": ""
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "[[ayame:colors.tag.hex]]"
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "[[ayame:colors.function.hex]]"
            }
        },
        {
            "name": "Object",
            "scope": [
                "variable.other.object"
            ],
            "settings": {
                "foreground": "[[ayame:colors.object.hex]]"
            }
        },
        {
            "name": "Block Level Variables",
            "scope": [
                "meta.block variable.other"
            ],
            "settings": {
                "foreground": "[[ayame:colors.foreground.hex]]"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": [
                "support.other.variable",
                "string.other.link"
            ],
            "settings": {
                "foreground": "[[ayame:colors.foreground.hex]]"
            }
        },
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "variable.parameter",
                "constant.character",
                "constant.escape",
                "keyword.other.unit",
                "keyword.other"
            ],
            "settings": {
                "foreground": "[[ayame:colors.constant.hex]]"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "[[ayame:colors.string.hex]]"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
